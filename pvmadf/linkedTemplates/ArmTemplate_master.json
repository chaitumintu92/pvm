{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "pvmadf"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureDatabricks1_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricks1'"
		},
		"AzureDatabricksDeltaLake1_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricksDeltaLake1'"
		},
		"SapOdp1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SapOdp1'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://pvm1d.dfs.core.windows.net/"
		},
		"AzureDatabricks1_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0217-085737-bljtoj77"
		},
		"SapOdp1_properties_typeProperties_clientId": {
			"type": "string",
			"defaultValue": "100"
		},
		"SapOdp1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "BPINST"
		},
		"SapOdp1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "34.194.47.247"
		},
		"SapOdp1_properties_typeProperties_systemNumber": {
			"type": "string",
			"defaultValue": "00"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "pvmadf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureDatabricks1_accessToken": {
						"value": "[parameters('AzureDatabricks1_accessToken')]"
					},
					"AzureDatabricksDeltaLake1_accessToken": {
						"value": "[parameters('AzureDatabricksDeltaLake1_accessToken')]"
					},
					"SapOdp1_password": {
						"value": "[parameters('SapOdp1_password')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureDatabricks1_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks1_properties_typeProperties_existingClusterId')]"
					},
					"SapOdp1_properties_typeProperties_clientId": {
						"value": "[parameters('SapOdp1_properties_typeProperties_clientId')]"
					},
					"SapOdp1_properties_typeProperties_userName": {
						"value": "[parameters('SapOdp1_properties_typeProperties_userName')]"
					},
					"SapOdp1_properties_typeProperties_server": {
						"value": "[parameters('SapOdp1_properties_typeProperties_server')]"
					},
					"SapOdp1_properties_typeProperties_systemNumber": {
						"value": "[parameters('SapOdp1_properties_typeProperties_systemNumber')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}